  {{#if include_tickets}}
    <div class="container tickets-area-container">
      <h1 class="ticket-area-title">Bilhetes</h1>
      <div class="row ticket-categories-row">
        <div class="ticket-eb col-sm-5 ticket-categories-col">
          <i class="circle"></i>
          <br/>
          <div class="ticket-box">
            {{#if earlybird_soldout}}
            <h3 class="text-strike">Early bird</h3>
            <p class="type-note">Esgotado!</p>
            {{else}}
            <h3>Early bird</h3>
            {{/if}}
            <div>
              <div>
                <span>Individual</span>
                {{#if earlybird_soldout}} {{else}}
                <span class="price">25€</span>
                {{/if}}
              </div>
              <div>
                <span>Grupo</span>
                {{#if earlybird_soldout}} {{else}}
                <span class="price">20€</span>
                {{/if}}
              </div>
            </div>
          </div>
        </div>

        <div class="ticket-n col-sm-offset-2 col-sm-5 ticket-categories-col">
          <i class="circle"></i>
          <br/>
          <div class="ticket-box">
            <h3>Regular</h3>
            {{#if earlybird_soldout}}
            <p class="transparent type-note">.</p>
            {{/if}}
            <div>
              <div>
                <span>Individual</span>
                <span class="price">30€</span>
              </div>
              <div>
                <span>Grupo</span>
                <span class="price">25€</span>
              </div>
            </div>
          </div>
        </div>
      </div>
      <a class="btn btn-buy-now" href="https://goo.gl/XvWmqU">Compra Já o Teu Bilhete</a>
    </div>
  {{/if}}

