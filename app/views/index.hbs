  {{#if desafios_enabled}}
  <div class="separator-row-secondary">
    <div class="row separator-shapes-2">
      <div class="col-xs-4 shape-square enei-skew enei-bgd-bgdblue anim-vertical-phase1"></div>
      <div class="col-xs-4 shape-square enei-skew enei-bgd-thrdhlghtblue anim-vertical-phase2"></div>
      <div class="col-xs-4 shape-square enei-skew enei-bgd-hlghtpurple anim-vertical-phase3"></div>
    </div>
  </div>

  <div id="faqs" class="container faqs-container">
    <h1 class="section-title" id="index-faq">Desafios do ENEI</h1>

    <div class="panel-group" id="desafios-accordion">
      {{#each desafios}}
      <div class="panel panel-default">
        <div class="panel-heading">
          <h4 class="panel-title">
            <a data-toggle="collapse" data-parent="#desafios-accordion" href="#faq-collapse-{{id}}">
              {{question}}</a>
          </h4>
        </div>
        <div id="faq-collapse-{{id}}" class="panel-collapse collapse">
          <div class="panel-body faq-answer">
            {{{answer}}}
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
  {{/if}}

  <div class="separator-row-secondary">
    <div class="row separator-shapes-2">
      <div class="col-xs-4 shape-square enei-skew enei-bgd-bgdblue anim-vertical-phase1"></div>
      <div class="col-xs-4 shape-square enei-skew enei-bgd-thrdhlghtblue anim-vertical-phase2"></div>
      <div class="col-xs-4 shape-square enei-skew enei-bgd-hlghtpurple anim-vertical-phase3"></div>
    </div>
  </div>

  <div id="faqs" class="container faqs-container">
    <h1 class="section-title" id="index-faq">FAQ's</h1>

    <div class="panel-group" id="accordion">
      {{#each faqs}}
      <div class="panel panel-default">
        <div class="panel-heading">
          <h4 class="panel-title">
            <a data-toggle="collapse" data-parent="#accordion" href="#faq-collapse-{{id}}">
              {{question}}</a>
          </h4>
        </div>
        <div id="faq-collapse-{{id}}" class="panel-collapse collapse">
          <div class="panel-body faq-answer">
            {{{answer}}}
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>

  {{#if include_sponsors}}
    <div class="sponsors-container">
      <div class="container">
        <h1 class="sponsor-title section-title">Patrocinadores</h1>

        <div class="sponsor main">
          {{#each sponsors.main}}
            {{#if show}}
              <a target="_blank" href="{{href}}">
                <img title="{{name}}" alt="{{name}}" src="images/main/{{image}}">
              </a>
            {{/if}}
          {{/each}}
        </div>
        <div class="sponsor gold">
          {{#each sponsors.gold}}
            {{#if show}}
              <a target="_blank" href="{{href}}">
                <img title="{{name}}" alt="{{name}}" src="images/gold/{{image}}">
              </a>
            {{/if}}
          {{/each}}
        </div>
        <div class="sponsor silver">
          {{#each sponsors.silver}}
            {{#if show}}
              <a target="_blank" href="{{href}}">
                <img title="{{name}}" alt="{{name}}" src="images/silver/{{image}}">
              </a>
            {{/if}}
          {{/each}}
        </div>
        <div class="container">
        <div class="sponsor bronze row">
          {{#each sponsors.bronze_level1}}
            {{#if show}}
            <a href="{{href}}">
            <div class="col-md-2" style="background-image: url('images/bronze/{{image}}')"></div>
            </a>
              <!--<a target="_blank" href="{{href}}">-->
                <!--<img title="{{name}}" alt="{{name}}" src="images/bronze/logos/{{image}}">-->
              <!--</a>-->
            {{/if}}
          {{/each}}
        </div>
        </div>
        <div class="container">
        <div class="sponsor bronze row ">
          {{#each sponsors.bronze_level2}}
            {{#if show}}
            <a href="{{href}}">
            <div class="col-md-2" style="background-image: url('images/bronze/{{image}}')"></div>
            </a>
            {{/if}}
          {{/each}}
        </div>
        </div>
      </div>
      <div class="container">
        <h1 class="sponsor-title section-title">Parceiros</h1>
        <div class="sponsor silver partner">
          {{#each partners1}}
            {{#if show}}
              <a target="_blank" href="{{href}}">
                <img title="{{name}}" alt="{{name}}" src="images/{{image}}">
              </a>
            {{/if}}
          {{/each}}
        </div>
        <div class="sponsor silver partner">
          {{#each partners2}}
            {{#if show}}
              <a target="_blank" href="{{href}}">
                <img title="{{name}}" alt="{{name}}" src="images/{{image}}">
              </a>
            {{/if}}
          {{/each}}
        </div>
      </div>
    </div>
  {{/if}}

</div>

